{
    "name": "hacker-prime/compuzign-artifactory-cli",
    "description": "A command-line interface (CLI) tool for managing JFrog Artifactory instances",
    "authors": [
        {
            "name": "Shayn Hacker",
            "email": "shaynhacker@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "^5.4",
        "guzzlehttp/guzzle": "^7.4",
        "symfony/deprecation-contracts": "^2.2",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "bin": [
        "bin/console"
    ],
    "scripts": {
        "build-phar": "sh scripts/build-phar.sh"
    },
    "require-dev": {
        "humbug/box": "^3.11"
    }
}
